{
  "name": "organ-finder-auth",
  "version": "1.0.0",
  "description": "Authentication system for Organ Finder",
  "main": "src/server/index.ts",
  "scripts": {
    "start": "ts-node src/server/index.ts",
    "dev:server": "nodemon src/server/index.ts",
    "dev:client": "cd src/client && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:server": "tsc",
    "build:client": "cd src/client && npm run build",
    "build": "npm run build:server && npm run build:client",
    "test": "cd src/client && npm test",
    "init-db": "node scripts/init-db.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.9.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
